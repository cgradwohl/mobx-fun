{
  "name": "app",
  "version": "0.0.01",
  "description": "starter client application",
  "main": "lib/app.js",
  "author": "Chris Gradwohl <christophergradwohl@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "node_modules/.bin/eslint src",
    "dev": "npx nodemon -w src/ -e js,scss -x \"rm -rf lib; mkdir lib; webpack --env dev && webpack --env build",
    "dev-test": "npx nodemon -w src/ -e js,scss -x",
    "build": "webpack --env dev && webpack --env build && yarn test",
    "test": "mocha --require @babel/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require @babel/register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/app.js)\""
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.2.0",
    "mobx-state-tree": "^3.14.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0"
  },
  "resolutions": {
    "**/react": "16.8.6",
    "**/react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "nyc": "^13.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "yargs": "^13.2.2"
  }
}
